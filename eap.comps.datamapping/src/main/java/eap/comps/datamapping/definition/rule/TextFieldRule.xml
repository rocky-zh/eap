<?xml version="1.0" encoding="UTF-8" ?>
<digester-rules>
	<pattern value="Field">
		<object-create-rule classname="eap.comps.datamapping.definition.TextFieldDefinition" />
		<set-properties-rule />
		<include path="eap/comps/datamapping/definition/rule/PropertyRule.xml"/>
		<include path="eap/comps/datamapping/definition/rule/ValidatorRule.xml"/>
		<include path="eap/comps/datamapping/definition/rule/RendererRule.xml"/>
		<include path="eap/comps/datamapping/definition/rule/HandlerRule.xml"/>
		<set-next-rule methodname="addField" />
	</pattern>
</digester-rules>